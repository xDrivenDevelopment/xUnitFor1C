			// TODO
			Сообщить(""Не удалось загрузить тесты из истории <"" + ЭлементИстории.ИдентификаторЗагрузчика + "": "" + ЭлементИстории.Путь + "">"" + Символы.ПС + ОписаниеОшибки());
